{
  "name": "signalium",
  "version": "2.1.5",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Signalium/signalium.git"
  },
  "description": "Scalable signals, effortless async. Reactivity at critical mass.",
  "main": "./dist/cjs/production/index.js",
  "module": "./dist/esm/production/index.js",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "development": {
        "import": "./dist/esm/development/index.js",
        "require": "./dist/cjs/development/index.js"
      },
      "production": {
        "import": "./dist/esm/production/index.js",
        "require": "./dist/cjs/production/index.js"
      },
      "import": "./dist/esm/production/index.js",
      "require": "./dist/cjs/production/index.js",
      "default": "./dist/esm/production/index.js"
    },
    "./config": {
      "types": "./dist/esm/config.d.ts",
      "development": {
        "import": "./dist/esm/development/config.js",
        "require": "./dist/cjs/development/config.js"
      },
      "production": {
        "import": "./dist/esm/production/config.js",
        "require": "./dist/cjs/production/config.js"
      },
      "import": "./dist/esm/production/config.js",
      "require": "./dist/cjs/production/config.js",
      "default": "./dist/esm/production/config.js"
    },
    "./utils": {
      "types": "./dist/esm/utils.d.ts",
      "development": {
        "import": "./dist/esm/development/utils.js",
        "require": "./dist/cjs/development/utils.js"
      },
      "production": {
        "import": "./dist/esm/production/utils.js",
        "require": "./dist/cjs/production/utils.js"
      },
      "import": "./dist/esm/production/utils.js",
      "require": "./dist/cjs/production/utils.js",
      "default": "./dist/esm/production/utils.js"
    },
    "./debug": {
      "types": "./dist/esm/debug.d.ts",
      "development": {
        "import": "./dist/esm/development/debug.js",
        "require": "./dist/cjs/development/debug.js"
      },
      "production": {
        "import": "./dist/esm/production/debug.js",
        "require": "./dist/cjs/production/debug.js"
      },
      "import": "./dist/esm/production/debug.js",
      "require": "./dist/cjs/production/debug.js",
      "default": "./dist/esm/production/debug.js"
    },
    "./react": {
      "types": "./dist/esm/react/index.d.ts",
      "development": {
        "import": "./dist/esm/development/react/index.js",
        "require": "./dist/cjs/development/react/index.js"
      },
      "production": {
        "import": "./dist/esm/production/react/index.js",
        "require": "./dist/cjs/production/react/index.js"
      },
      "import": "./dist/esm/production/react/index.js",
      "require": "./dist/cjs/production/react/index.js",
      "default": "./dist/esm/production/react/index.js"
    },
    "./transform": {
      "types": "./dist/esm/transform/index.d.ts",
      "development": {
        "import": "./dist/esm/development/transform/index.js",
        "require": "./dist/cjs/development/transform/index.js"
      },
      "production": {
        "import": "./dist/esm/production/transform/index.js",
        "require": "./dist/cjs/production/transform/index.js"
      },
      "import": "./dist/esm/production/transform/index.js",
      "require": "./dist/cjs/production/transform/index.js",
      "default": "./dist/esm/production/transform/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/esm/**",
    "dist/cjs/**",
    "react.js",
    "transform.js",
    "debug.js",
    "utils.js",
    "config.js",
    "README.md",
    "CHANGELOG.md",
    "LICENSE*"
  ],
  "peerDependencies": {
    "react": ">=18.3.1"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "scripts": {
    "dev": "vitest dev",
    "dev:unit": "vitest dev --project unit",
    "dev:transform": "vitest dev --project transform",
    "dev:react": "vitest dev --project react",
    "test": "vitest run",
    "test:unit": "vitest run --project unit",
    "test:transform": "vitest run --project transform",
    "test:react": "vitest run --project react",
    "check-types": "tsc --noEmit",
    "build": "npm run build:clean && npm run build:types && npm run build:dev && npm run build:prod && node scripts/generate-legacy-entries.cjs",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/esm",
    "build:dev": "BUILD_MODE=development vite build",
    "build:prod": "BUILD_MODE=production vite build"
  },
  "author": "Kristen Hewell Garrett <me@pzuraq.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "^3.0.6",
    "playwright": "^1.50.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^7.1.2",
    "vite-plugin-babel": "^1.3.0",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.1"
  }
}
